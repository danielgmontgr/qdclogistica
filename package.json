{
  "name": "qdclogistica",
  "productName": "QDClogistica",
  "version": "1.0.0",
  "description": "Quiero de Comer Log√≠stica de Pedidos",
  "main": "src/main.js",
  "urlapi": "http://k7.grupored.mx/QdcApiQas/",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=Images/iconapp.ico",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "daniel.gonzalez",
  "license": "GRUPO RED",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/Images/iconapp.png"
      },
      "electronWinstallerConfig": {
        "name": "QDClogistica",
        "icon": "./src/Images/appicon.ico",
        "setupIcon": "./src/Images/appicon.ico",
        "loadingGif": "./src/Images/loading.gif"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "QDClogistica"
      }
    }
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "auto-launch": "^5.0.5",
    "bootstrap": "^4.3.1",
    "electron-compile": "^6.4.4",
    "electron-is-dev": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "excellentexport": "^3.3.0",
    "got": "^9.6.0",
    "inputmask": "^4.0.6",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "tether": "^1.4.0",
    "update-electron-app": "^1.3.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "appicon.ico",
      "uninstallerIcon": "appicon.ico"
    },
    "win": {
      "target": "nsis",
      "icon": "./src/Images/appicon.ico"
    }
  }
}
